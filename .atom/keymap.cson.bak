"atom-workspace, atom-text-editor":
  "f12": "window:toggle-dev-tools"
  "home": "editor:move-to-first-character-of-line"
  "end": "editor:move-to-end-of-line"
  "left":"core:move-left"
  "right":"core:move-right"
  "pageup": "half-pager:half-page-up"
  "up":"core:move-up"
  "down":"core:move-down"
  "pagedown": "half-pager:half-page-down"
  "shift-space": "spell-check:correct-misspelling"
  "alt-f5": "window:reload"
  "alt-up": "editor:add-selection-above"
  "alt-down": "editor:add-selection-below"
  "ctrl-`": "settings-view:open"
  "ctrl-=": "tree-view:toggle-focus"
  "ctrl-q": "pane:reopen-closed-item"
  "ctrl-g": "go-to-line:toggle"
  "ctrl-u": "bracket-matcher:remove-brackets-from-selection"
  "ctrl-p": "project-folder:add"
  "ctrl-j": "bracket-matcher:go-to-matching-bracket"
  "ctrl-x": "core:cut"
  "ctrl-l": "editor:select-line"
  "ctrl-d": "find-and-replace:select-next"
  "ctrl-c": "core:copy"
  "ctrl-w": "core:close"
  "ctrl-'": "toggle-quotes:toggle"
  "ctrl-enter": "editor:duplicate-lines"
  "ctrl-a": "core:select-all"
  "ctrl-o": "editor:upper-case"
  "ctrl-e": "editor:lower-case"
  "ctrl-i": "bracket-matcher:select-inside-brackets"
  "ctrl-y": "core:redo"
  "ctrl-f": "find-and-replace:use-selection-as-find-pattern"
  "ctrl-h": "recent-finder:toggle"
  "ctrl-t": "fuzzy-finder:toggle-file-finder"
  "ctrl-s": "core:save"
  "ctrl-r": "command-palette:toggle"
  "ctrl-tab": "pane:show-next-item"
  "ctrl-backspace": "editor:delete-to-beginning-of-word"
  "ctrl-\\": "regex-railroad-diagram:toggle"
  "ctrl-/": "editor:toggle-line-comments"
  "ctrl--": "toggle:there"
  "ctrl-b": "snippets:available"
  "ctrl-k": "grammar-selector:show"
  "ctrl-v": "core:paste"
  "ctrl-m": "lazy-motion:forward-again"
  "ctrl-,": "find-and-replace:show"
  "ctrl-.": "project-find:show"
  "ctrl-;": "key-binding-resolver:toggle"
  "ctrl-delete": "editor:delete-to-end-of-word"
  "ctrl-space": "emacs:set-mark"
  "ctrl-n": "advanced-new-file:toggle"
  "ctrl-z": "core:undo"
  "ctrl-[": "editor:outdent-selected-rows"
  "ctrl-]": "editor:indent-selected-rows"
  "ctrl-home": "core:move-to-top"
  "ctrl-end": "core:move-to-bottom"
  "ctrl-left":"editor:move-to-beginning-of-word"
  "ctrl-right":"editor:move-to-beginning-of-next-word"
  "ctrl-pageup": "indentation-jumper:up"
  "ctrl-up":"editor:move-line-up"
  "ctrl-down":"editor:move-line-down"
  "ctrl-pagedown": "indentation-jumper:down"
  "ctrl-shift-tab": "pane:show-previous-item"
  "ctrl-alt-2": "paner:very-left"
  "ctrl-alt-3": "paner:very-right"
  "ctrl-alt-4": "window:focus-previous-pane"
  "ctrl-alt-5": "window:focus-next-pane"
  "ctrl-alt-6": "pane:split-right-moving-current-tab"
  "ctrl-alt-7": "pane:split-right"
  "ctrl-alt-8": "paner:very-top"
  "ctrl-alt-9": "paner:very-bottom"
  "ctrl-alt-u": "source-preview:toggle"
  "ctrl-alt-l": "linter:toggle"
  "ctrl-alt-d": "hashrocket:run"
  "ctrl-alt-c": "run:stop"
  "ctrl-alt-w": "pane:close"
  "ctrl-alt-e": "line-ending-converter:convert-to-unix-format"
  "ctrl-alt-i": "minimap:toggle"
  "ctrl-alt-t": "tabs-to-spaces:untabify"
  "ctrl-alt-s": "core:save-as"
  "ctrl-alt-r": "run:file"
  "ctrl-alt-b": "atom-html-preview:toggle"
  "ctrl-alt-k": "css-comb:run"
  "ctrl-alt-v": "try:paste"
  "ctrl-alt-m": "markdown-preview:toggle"
  "ctrl-alt-n": "pane:split-right-creating-empty-pane"
  "ctrl-alt-z": "editor:checkout-head-revision"
  "ctrl-alt-[": "resize-indent:halve"
  "ctrl-alt-]": "resize-indent:double"
  "ctrl-alt-E": "line-ending-converter:convert-to-windows-format"
  "ctrl-alt-T": "tabs-to-spaces:tabify"

"atom-text-editor.transient-marked":
  "home": "editor:select-to-first-character-of-line"
  "end": "editor:select-to-end-of-line"
  "left":"core:select-left"
  "right":"core:select-right"
  "pageup": "half-pager:select-half-page-up"
  "up":"core:select-up"
  "down":"core:select-down"
  "pagedown": "half-pager:select-half-page-down"
  "ctrl-x": "emacs:kill"
  "ctrl-c": "emacs:copy-region"
  "ctrl-home": "core:select-to-top"
  "ctrl-end": "core:select-to-bottom"
  "ctrl-left":"editor:select-to-beginning-of-word"
  "ctrl-right":"editor:select-to-beginning-of-next-word"
  "ctrl-pageup": "indentation-jumper:select-up"
  "ctrl-pagedown": "indentation-jumper:select-down"

"atom-text-editor.conflicted":
  "ctrl-alt-,": "merge-conflicts:previous-unresolved"
  "ctrl-alt-.": "merge-conflicts:next-unresolved"

"atom-text-editor:not(.conflicted)":
  "ctrl-alt-,": "linter:previous-error"
  "ctrl-alt-.": "linter:next-error"

"atom-text-editor.lazy-motion":
  "enter": "lazy-motion:forward"
  "shift-enter": "lazy-motion:backward"
  "tab": "core:confirm"

"atom-workspace atom-text-editor:not([mini])":
  "ctrl-enter": "unset!"
  "ctrl-enter": "editor:duplicate-lines"
  "ctrl-alt-[": "unset!"
  "ctrl-alt-[": "resize-indent:halve"
  "ctrl-alt-]": "unset!"
  "ctrl-alt-]": "resize-indent:double"

".platform-win32 atom-text-editor":
  "ctrl-r": "unset!"
  "ctrl-r": "command-palette:toggle"
